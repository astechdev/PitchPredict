	function update_single_database_item_single_parameter($tableName, $firstColumnName, $firstColumnElement, $firstParameterName, $firstParameterColumnElement)
	{
		$update = "UPDATE $tableName SET $firstColumnName = '$firstColumnElement' WHERE $firstParameterName = '$firstParameterColumnElement'";
		mysql_query($update) or die('Error updating '.mysql_error());
	}
