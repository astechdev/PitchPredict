	function insert_single_database_item($tableName, $firstColumnName, $firstElement)
	{
		$insert = "INSERT INTO $tableName ($firstColumnName) VALUES ('$firstElement')";
		$result = mysql_query($insert);

		if(!$result)
		{
			echo "insert: $insert";
			throw new Exception('Mysql Error. '.mysql_error());
		}
	}

