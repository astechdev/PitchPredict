				function get_counter_four_columns($primaryKeyName, $tableName, $firstColumnName, $firstElement, $secondColumnName, $secondElement, $thirdColumnName, $thirdElement, $fourthColumnName, $fourthElement)

					{
						$query = "SELECT $primaryKeyName FROM $tableName WHERE ($firstColumnName = '$firstElement' AND $secondColumnName = '$secondElement' AND $thirdColumnName = '$thirdElement' AND  $fourthColumnName = '$fourthElement')";

						$result = mysql_query($query);

						if(!$result)
						{
							echo "query: $query";
							throw new Exception('Mysql Error. '.mysql_error());
						}

						$counterValue = 0;

						while($row = mysql_fetch_array($result, MYSQL_ASSOC))
						{
							$primaryKeyValue = $row[$primaryKeyName];
							//echo "Counter Value: $primaryKeyValue<br>";
							$counterValue = $counterValue + $primaryKeyValue;
							//echo "Counter Value: $counterValue<br>";
						}
/*
						if($counterValue == 0)
						{
							echo "counterValue == 0,<br> query: $query";
			             	throw new Exception('Mysql returned 0 results.');
						}
*/
						return $counterValue;
					}

